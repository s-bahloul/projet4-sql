         const productsForCategory = new Set(\n                        arrayOfTargetedProducts.filter((array) => {\n                            return catObj.categoryValues.includes(array[0].category);\n                        })\n                    );\n\n                    const revenue = [...productsForCategory].reduce((acc, currValue) => {\n                        return acc + currValue[0].amount;\n                    }, 0);\n\n                    if (revenue > 0 && !executedGoalsArray.includes(catObj.goalName)) {\n                        Kameleoon.API.Goals.processConversion(goals[catObj.goalName], revenue);\n                        executedGoalsArray.push(catObj.goalName);\n                        if (catObj.extraGoal) {\n                            Kameleoon.API.Goals.processConversion(goals[catObj.extraGoal]);\n\n                            if (!executedExtraGoalsArray.includes(catObj.goalName)) {\n                                executedExtraGoalsArray.push(catObj.extraGoal);\n                            }\n\n                            if (!orderedProducts) {\n                                Kameleoon.API.Data.writeLocalData('kamDev-strateHP-categoryGoals', executedExtraGoalsArray, true);\n                            } else if (!orderedProducts.includes(catObj.extraGoal)) {\n                                orderedProducts.push(catObj.extraGoal);\n                                Kameleoon.API.Data.writeLocalData('kamDev-strateHP-categoryGoals', orderedProducts, true);\n                            }\n                        }\n                        return;\n                    }\n                });\n            };\n\n            const setGoalWithRevenue = () => {\n                Kameleoon.API.Core.runWhenConditionTrue(\n                    () => {\n                        const isDataLayerProductsLoaded = window.tc_vars && tc_vars.order_products.length;\n\n                        return isDataLayerProductsLoaded;\n                    },\n                    () => {\n                        const orderedProducts = tc_vars.order_products;\n                        triggerGoalByCategory(orderedProducts);\n                    }\n                );\n            };\n\n            window.location.pathname.match('/orderpipe/pop/merci') ? setGoalWithRevenue() : setStorageItems();\n        };\n\n        setupGoals();\n    })();\n    */\n    // END OF [FNAC_DARTY] Setup Goals Fnac IA\n\n    // Setup API Product\n    (function () {\n        const getProductApiObject = () => {\n            return {\n                name: window.tc_vars.product_name,\n                category: window.tc_vars.arbo_niv1,\n                mainImageURL: window.tc_vars.product_picture_url,\n                category: window.tc_vars.product_category\n                //Baisse de Prix additions\n                // price: window.tc_vars.product_unitprice_discount,\n                //categoryID: window.tc_vars.product_unitprice,\n            };\n        };\n\n        Kameleoon.API.Core.runWhenConditionTrue(\n            () => window.tc_vars,\n            () => {\n                const productApiObject = getProductApiObject();\n                if (window.tc_vars.product_id && window.tc_vars.product_unitprice_ati) {\n                    Kameleoon.API.Products.trackProductView(window.tc_vars.product_id, productApiObject);\n                    Kameleoon.API.Products.trackProductView(window.tc_vars.product_id, {\n                        price: window.tc_vars.product_unitprice_ati,\n                    });\n                }\n                //Baisse de Prix additions\n                /*\n                if (window.tc_vars.user_segment === 'Non-Adherent') {\n                    Kameleoon.API.Products.trackProductView(window.tc_vars.product_id, {\n                        price: window.tc_vars.product_unitprice_discount,\n                    });\n                }*/\n            }\n        );\n    })();\n    //END Setup API Product\n\n    //START Baisse de Prix\n    const BDPsearch = () => {\n        function callback() {\n            const tabletWidth = 768;\n            const productCategory = 'Enfants, jouets';\n            const regexpStatus = /neufs à partir de|occasions dès|Plus d'offres/i;\n            const discountRange = 259200000;\n            const millisecInHour = 3600000;\n            const discountSelector = '.oldPrice';\n            const isDesktop = innerWidth >= tabletWidth;\n            let selector;\n            let state;\n\n            function checkForConditions() {\n                Kameleoon.API.Core.runWhenElementPresent(selector, (articles) => {\n                    const listOfProperIds = [];\n                    window.bdp_search = {};\n                    articles.forEach((productTile, index) => {\n                        const conditionCrossedPrice = !productTile.querySelector(discountSelector);\n                        let condition11;\n                        productTile.setAttribute('kamflag', 'true');\n\n                        if (isDesktop) {\n                            if (state === 'mosaic') {\n                                condition11 = productTile.querySelector('.more')\n                                    && regexpStatus.test(productTile.querySelector('.more').textContent);\n                            } else {\n                                condition11 = productTile.querySelector('.f-buyBox-availabilityStatus-available');\n                            }\n                        } else if (state === 'mosaic') {\n                            condition11 = productTile.querySelector('.soon');\n                        } else {\n                            condition11 = productTile.querySelector('.shop-availability.soon');\n                        }\n                        const elementWithId = productTile.querySelector('[data-prid]');\n\n                        // For QA FR\n                        window.bdp_search[index] = {\n                            condition11: condition11,\n                            condition_not_crossed_price: conditionCrossedPrice,\n                            condition_price_has_decreased_in_the_last_72h: null\n                        };\n                        if (\n                            condition11\n                            && elementWithId\n                            && conditionCrossedPrice\n                        ) {\n                            const id = elementWithId.getAttribute('data-prid');\n                            listOfProperIds.push(id);\n                            window.bdp_search[index].id = id;\n                        }\n\n                    });\n                    if (listOfProperIds.length) {\n                        fetch(`https://api-product.kameleoon.com/productData?siteCode=ds1h54b9h1&eans=${JSON.stringify(Array.from(listOfProperIds))}&all=true`)\n                            .then((response) => response.json())\n                            .then((response) => Object.entries(response).reduce((accumulator, value) => {\n                                const element = value[1];\n                                const price = parseInt(element.categoryId);\n\n                                const timestamp = element.timesAndPricesSortedAscTime\n                                    && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1]\n                                    && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1][0];\n                                const reducedPrice = element.timesAndPricesSortedAscTime\n                                    && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1]\n                                    && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1][1];\n\n\n                                const isCategoryMatches = element.category === productCategory;\n                                const isPriceChanged = price - reducedPrice > 1;\n                                const isPriceReducedLast72h = (new Date() - new Date(timestamp)) / millisecInHour < discountRange;\n                                const id = value[0];\n\n                                if (timestamp && isCategoryMatches && isPriceChanged && isPriceReducedLast72h) {\n                                    accumulator.push(id);\n                                }\n\n                                if (isPriceChanged && isPriceReducedLast72h) {\n                                    Object.entries(window.bdp_search).find((el) => {\n                                        if (el[1].id === id) {\n                                            el[1].condition_price_has_decreased_in_the_last_72h = true;\n                                        }\n                                    });\n                                }\n                                return accumulator;\n                            }, []))\n                            .then((response) => {\n                                if (response && response.length) {\n                                    sessionStorage.setItem('KamDev_BaisseDePrix_Search&List', response);\n                                    Kameleoon.API.Events.trigger('BaisseDePrix_search');\n                                }\n                                Kameleoon.API.Core.runWhenConditionTrue(\n                                    () => {\n                                        const mosaicSelector = isDesktop ? '.Article-mosaicItem' : '.f-searchResult';\n                                        const listSelector = isDesktop ? '.Article-item' : '.f-searchResult';\n                                        return document.querySelector(`${mosaicSelector}:not([kamflag])`)\n                                            || document.querySelector(`${listSelector}:not([kamflag])`);\n                                    },\n                                    () => {\n                                        callback();\n                                    },\n                                    1000\n                                );\n                            });\n                    }\n                });\n            }\n            // Check for proper selectors\n            if (isDesktop) {\n                Kameleoon.API.Core.runWhenElementPresent('a.active',\n                    ([viewState]) => {\n                        if (/list/i.test(viewState.className)) {\n                            selector = '.Article-item';\n                            state = 'list';\n                            checkForConditions();\n                        } else {\n                            selector = '.Article-mosaicItem';\n                            state = 'mosaic';\n                            checkForConditions();\n                        }\n                    });\n            } else {\n                Kameleoon.API.Core.runWhenElementPresent('.js-searchView',\n                    ([viewState]) => {\n                        if (/to_mosaic/i.test(viewState.className)) {\n                            selector = '.f-searchResult';\n                            state = 'list';\n                            checkForConditions();\n                        } else {\n                            selector = '.f-searchResult';\n                            state = 'mosaic';\n                            checkForConditions();\n                        }\n                    });\n            }\n        }\n        // Implementation\n        callback();\n    };\n\n    const BDPpdp = () => {\n        const tabletWidth = 768;\n        const productCategory = 'Enfants, jouets';\n        const regexpStatus = /En Stock/i;\n        const discountRange = 259200000;\n        const millisecInHour = 3600000;\n        const isDesktop = innerWidth >= tabletWidth;\n        const selector = isDesktop ? '.f-buyBox-availabilityStatus-available' : '.dispo';\n        const discountSelector = isDesktop ? '.f-priceBox-price--old' : '.userPrice--tempForStrikePrice';\n\n        function callback() {\n            Kameleoon.API.Core.runWhenElementPresent(selector, ([status]) => {\n                // Targeting conditions\n                const condition9 = Kameleoon.API.CurrentVisit.customData['Catégorie produit'] === productCategory;\n                const condition12 = regexpStatus.test(status.textContent);\n                const conditionCrossedPrice = !document.querySelector(discountSelector);\n\n                // For QA FR\n                window.bdp_pdp = {\n                    condition9: condition9,\n                    condition12: condition12,\n                    condition_not_crossed_price: conditionCrossedPrice,\n                    condition_price_has_decreased_in_the_last_72h: null\n                };\n\n                if (\n                    condition9\n                    && condition12\n                    && conditionCrossedPrice\n                ) {\n                    fetch(`https://api-product.kameleoon.com/productData?siteCode=ds1h54b9h1&eans=[\"${window.tc_vars.product_id}\"]&all=true`)\n                        .then((response) => response.json())\n                        .then((response) => {\n                            const data = Object.values(response)[0];\n                            const price = parseInt(data.categoryId);\n                            const timestamp = data.timesAndPricesSortedAscTime\n                                && data.timesAndPricesSortedAscTime[data.timesAndPricesSortedAscTime.length - 1]\n                                && data.timesAndPricesSortedAscTime[data.timesAndPricesSortedAscTime.length - 1][0];\n                            const reducedPrice = data.timesAndPricesSortedAscTime\n                                && data.timesAndPricesSortedAscTime[data.timesAndPricesSortedAscTime.length - 1]\n                                && data.timesAndPricesSortedAscTime[data.timesAndPricesSortedAscTime.length - 1][1];\n\n                            const hasPrevPrice = price - reducedPrice > 1;\n                            const isPriceReducedLast72h = (new Date() - new Date(timestamp)) / millisecInHour < discountRange;\n\n\n                            if (hasPrevPrice && isPriceReducedLast72h) {\n                                sessionStorage.setItem('KamDev_BaisseDePrix_PDP', window.tc_vars.product_id);\n                                Kameleoon.API.Events.trigger('BaisseDePrix_pdp');\n                                window.bdp_pdp.condition_price_has_decreased_in_the_last_72h = true;\n                            } else {\n                                window.bdp_pdp.condition_price_has_decreased_in_the_last_72h = false;\n                            }\n                        });\n                }\n            });\n        }\n        // Implementation\n        Kameleoon.API.Core.runWhenConditionTrue(\n            () => window.tc_vars && window.tc_vars.product_id,\n            callback\n        );\n    };\n\n    const BDPslider = () => {\n        const tabletWidth = 768;\n        const productCategory = 'Enfants, jouets';\n        const regexpAchorId = /\\/a(\\d+)\\//i;\n        const discountRange = 259200000;\n        const millisecInHour = 3600000;\n        const isDesktop = innerWidth >= tabletWidth;\n        const unitSlider = isDesktop ? '.thumbnail-content' : '.mini-FA';\n        const unitSliderOther = `div[class*=Carousel] ${unitSlider}`;\n        const discountSelector = isDesktop ? '.thumbnail-priceOld' : '.oldPrice';\n\n        const checkForSelector = () => {\n            if (/\\d{7}\\//.test(location.href)) return unitSlider;\n            return unitSliderOther;\n        };\n\n        function callback() {\n            const listOfProperIds = [];\n            window.bdp_slider = {};\n            Kameleoon.API.Core.runWhenConditionTrue(\n                () => document.querySelectorAll(checkForSelector()).length,\n                () => {\n                    document.querySelectorAll(checkForSelector()).forEach((sliderProduct, index) => {\n                        const condition13 = !sliderProduct.querySelector('.miniFA__seller');\n                        const conditionCrossedPrice = !sliderProduct.querySelector(discountSelector);\n                        const anchor = sliderProduct.querySelector('.mini-FA .title a')\n                            && sliderProduct.querySelector('.mini-FA .title a').getAttribute('href');\n                        const id = isDesktop\n                            ? sliderProduct.getAttribute('data-prid')\n                            : anchor\n                            && anchor.match(regexpAchorId)\n                            && anchor.match(regexpAchorId)[1];\n\n                        // For QA Fr\n                        window.bdp_slider[index] = {\n                            condition13: condition13,\n                            id: id,\n                            condition_not_crossed_price: conditionCrossedPrice,\n                            condition_price_has_decreased_in_the_last_72h: null\n                        };\n\n                        if (condition13\n                            && id\n                            && conditionCrossedPrice\n                        ) {\n                            listOfProperIds.push(id);\n                        }\n                    });\n\n                }\n            );\n\n            Kameleoon.API.Core.runWhenConditionTrue(\n                () => listOfProperIds.length,\n                () => {\n                    fetch(`https://api-product.kameleoon.com/productData?siteCode=ds1h54b9h1&eans=${JSON.stringify(Array.from(listOfProperIds))}&all=true`)\n                        .then((response) => response.json())\n                        .then((response) => Object.entries(response).reduce((accumulator, value) => {\n                            const element = value[1];\n                            const price = parseInt(element.categoryId);\n                            const timestamp = element.timesAndPricesSortedAscTime\n                                && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1]\n                                && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1][0];\n                            const reducedPrice = element.timesAndPricesSortedAscTime\n                                && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1]\n                                && element.timesAndPricesSortedAscTime[element.timesAndPricesSortedAscTime.length - 1][1];\n\n                            const isCategoryMatches = element.category === productCategory;\n                            const isPriceChanged = price - reducedPrice > 1;\n                            const isPriceReducedLast72h = (new Date() - new Date(timestamp)) / millisecInHour < discountRange;\n                            const id = value[0];\n\n                            if (timestamp && isCategoryMatches && isPriceChanged && isPriceReducedLast72h) {\n                                accumulator.push(value[0]);\n                            }\n\n                            if (isPriceChanged && isPriceReducedLast72h) {\n                                Object.entries(window.bdp_slider).find((el) => {\n                                    if (el[1].id === id) {\n                                        el[1].condition_price_has_decreased_in_the_last_72h = true;\n                                    }\n                                });\n                            }\n\n                            return accumulator;\n                        }, []))\n                        .then((response) => {\n                            if (response && response.length) {\n                                sessionStorage.setItem('KamDev_BaisseDePrix_slider', response);\n                                Kameleoon.API.Events.trigger('BaisseDePrix_slider');\n                            }\n                        });\n                }\n            );\n\n        }\n        // Implementation\n        setTimeout(() => {\n            callback();\n        }, 0);\n    };\n\n    const BDPgoals = () => {\n        Kameleoon.API.Core.runWhenConditionTrue(\n            () => window.tc_vars &&\n                window.tc_vars.template_type,\n            callback\n        );\n        function callback() {\n            const goals = {\n                'Liste - ajout panier new': 229106,\n                'Page produit - ajout panier new': 229107,\n                'Search Ajout Panier new': 229118,\n            };\n            //Selectors\n            const selectorListAndSearch = '.js-ProductBuy-add';\n            const selectorPDP = innerWidth > 768 ? '.ff-button.js-ProductBuy-add' : '.js-ProductBuy-add';\n            //Conditions\n            const pageType = window.tc_vars.template_type;\n            const conditionList = pageType === 'sous_home';\n            const conditionPDP = pageType === 'fiches';\n            const conditionSearch = pageType === 'moteur';\n            //Implementation\n            triggerGoal(conditionList, selectorListAndSearch, goals['Liste - ajout panier new']);\n            triggerGoal(conditionPDP, selectorPDP, goals['Page produit - ajout panier new']);\n            triggerGoal(conditionSearch, selectorListAndSearch, goals['Search Ajout Panier new']);\n        }\n        function triggerGoal(condition, selector, goal) {\n            if (condition) {\n                document.addEventListener('mousedown', ({ target }) => {\n                    if (target.closest(selector)) {\n                        Kameleoon.API.Goals.processConversion(goal);\n                    }\n                });\n            }\n        }\n    };\n\n    //Baisse de Prix Slider\n    document.onreadystatechange = function () {\n        if (document.readyState === 'complete') {\n            BDPslider();\n    