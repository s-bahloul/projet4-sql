          arhtml='<a class="ek-aricon ek-android" data-eklink="hotspot_webar" href="intent://arvr.google.com/scene-viewer/1.0?file=https://d15mv1adrb1s6e.cloudfront.net/product_bundles/GLB/'+arid+'.glb?link='+encodeURIComponent(uri)+'&title=#Intent;scheme=https;package=com.google.ar.core;action=android.intent.action.VIEW;S.browser_fallback_url='+encodeURIComponent(uri)+'%23model-viewer-no-ar-fallback;end;" style="max-height:none!important; max-width: 60px; background: rgba(0, 0, 0, 0) no-repeat scroll -9999px -9999px;  position:absolute"><img id="flix_key_features2" class="'+defaulticon+'" src="'+hsimg+'" style="width:60px; height:auto!important" alt="img" /></a>'
          //AddType model/vnd.pixar.usd usdz


          FlixjQ('.flix_hotspotV2').append(arhtml);
          
          
      }
      
