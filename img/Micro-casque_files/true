      FlixjQ('#flix_hotspots .flix_hs,#flix_hotspots .ek-desktop').on({
        click: function() {
                          var id = this.id.replace(settings[0].prefix, ""), flix_hotspot;
              if(hotspots[id]){
                  var html = hotspots[id].html;
                  setTimeout(function() {
                      flixhotspot.flixBox(html, id);
                  }, 100);
                  flixhotspot.log("media", id);
              }
          }
      });

      var tooltipDelayFncount =0;
      function tooltipDelayFn() { 
         
        if( FlixjQ('.flix_hs').length > 0){
         
          FlixjQ('.flix_hs' ).tooltip({
              position: {
                  my: "center bottom-20",
                  at: "center top",
                  using: function( position, feedback ) {
                      FlixjQ( this ).css( position );
                      FlixjQ( "<div>" )
                          .addClass( "flix_arrow" )
                          .addClass( feedback.vertical )
                          .addClass( feedback.horizontal )
                          .appendTo( this );
                  }
              }
          });
          FlixjQ('.flix_hs' ).tooltip( "option", "hide", { effect: "fade", duration: 100 } );
          tooltipDelayFncount=22
        } 
        else {
          if(tooltipDelayFncount < 20){
            setTimeout(function(){
              tooltipDelayFn();
              tooltipDelayFncount ++;
            },1000);
          }
        }
      }
      tooltipDelayFn();

      //log first load
      if(FlixjQ('#flix_hotspots') && FlixjQ('#flix_hotspots:not(.ek_hotspots) .flix_hs').length>0) {
