          return json_price || false;
        }*/
        if ( !! bread) {
          crumbs = bread.textContent.trim().match(/[\d\.\,]+/).pop().replace(/\,/g,"");
          return crumbs || false;
        }
      } 
      catch (e) {
      }
  },br:function (){
